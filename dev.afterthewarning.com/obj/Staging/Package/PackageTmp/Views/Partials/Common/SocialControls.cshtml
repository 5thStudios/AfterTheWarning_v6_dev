@using Models;
@inherits UmbracoViewPage


@{ 
    //Obtain facebook url
    string facebookUrl = Umbraco.TypedContent((int)Common.siteNode.Donate).GetPropertyValue<string>(Common.NodeProperties.facebookUrl);
}

<div class="socialControls">
    <a href="PRINT" onclick="window.print(); return false;" class="show-for-large">
        <img alt="print" src="~/images/common/printericon.png" />
    </a>

    <a href="@facebookUrl" target="_blank">
        <img alt="Facebook" src="~/images/common/fbicon.png" />
    </a>
</div>